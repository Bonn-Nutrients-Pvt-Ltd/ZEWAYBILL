{
  "_version": "1.65.0",
  "sap.app": {
    "id": "zirnview",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.17.3",
      "toolsId": "add271e1-e16c-4e60-91a3-fdfbee487d3d"
    },
    "dataSources": {
      "ZSB_ZEWAYBILL_VAN": {
        "uri": "/sap/opu/odata/IWFND/CATALOGSERVICE;v=2/Annotations(TechnicalName='ZSB_ZEWAYBILL_VAN',Version='0001')/$value/",
        "type": "ODataAnnotation",
        "settings": {
          "localUri": "localService/mainService/ZSB_ZEWAYBILL_VAN.xml"
        }
      },
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZSB_ZEWAYBILL/",
        "type": "OData",
        "settings": {
          "annotations": [
            "ZSB_ZEWAYBILL_VAN"
          ],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "ZIRN-Display": {
          "semanticObject": "ZIRN",
          "action": "Display",
          "title": "{{ZIRN-Display.flpTitle}}",
          "subTitle": "{{ZIRN-Display.flpSubtitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.130.8",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "zirnview.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "zirnview.view",
        "async": true,
        "viewPath": "zirnview.view"
      },
      "routes": [
        {
          "name": "RouteGrid",
          "pattern": ":?query:",
          "target": [
            "TargetGrid"
          ]
        }
      ],
      "targets": {
        "TargetGrid": {
          "id": "Grid",
          "name": "Grid"
        }
      }
    },
    "rootView": {
      "viewName": "zirnview.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
